    <validationRules>
        <fullName>Commerce_Jet_Capacity_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>(RecordType.DeveloperName = &quot;Commerce&quot;) &amp;&amp; (Required_Capacity__c &gt; Jet__r.Capacity__c)</errorConditionFormula>
        <errorDisplayField>Jet__c</errorDisplayField>
        <errorMessage>Capacity of the selected Jet is not enough!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Commerce_Option_On</fullName>
        <active>true</active>
        <errorConditionFormula>(RecordType.DeveloperName = &quot;Commerce&quot;) &amp;&amp; ISBLANK(Required_Capacity__c)</errorConditionFormula>
        <errorDisplayField>Required_Capacity__c</errorDisplayField>
        <errorMessage>You need to specify Capacity for Commerce booking</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Customer_Age_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>Customer__r.Age__c  &lt; 21</errorConditionFormula>
        <errorDisplayField>Customer__c</errorDisplayField>
        <errorMessage>Customer is not adult</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Jet_Weight_Validation</fullName>
        <active>true</active>
        <description>Validation rule to ensure that airports runway is capable for the mass of the jet</description>
        <errorConditionFormula>Jet__r.Weight__c  &gt;  Arival_Airport__r.Runway_Max_Weight_Capacity__c</errorConditionFormula>
        <errorDisplayField>Arival_Airport__c</errorDisplayField>
        <errorMessage>Selected Arival Airport is not capable of receiving jet with such mass. Please select lighter Jet or more capable Airport</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pilot_Flied_Hours_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>Customer__r.Total_Flied_Hours__c  &lt; 20</errorConditionFormula>
        <errorDisplayField>Customer__c</errorDisplayField>
        <errorMessage>Current pilot has not enough total flied hours</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
